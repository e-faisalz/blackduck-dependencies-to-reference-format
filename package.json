{
  "name": "blackduck-dependencies-to-reference-format",
  "version": "1.0.2",
  "description": "Extracts dependencies from the output (source csv) of the Detect tool from BlackDuck (Synopsis), to a reference format. This reference format is a JSON file containing arrays of objects with keys name and version.",
  "main": "index.js",
  "scripts": {
    "lint": "yarn standard",
    "lint:fix": "yarn standard --fix",
    "test": "jest src --passWithNoTests",
    "coverage": "jest --coverage src",
    "extract-from-source": "./src/cli-extract-from-source.js"
  },
  "author": "Sanda Contiu",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "jest": "^24.1.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "csvtojson": "^2.0.8",
    "fs-extra": "^7.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**.js",
      "src/**/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/cli-extarct-from-source.js",
      "!**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/public/**"
    ],
    "envs": [
      "es6",
      "browser",
      "jest"
    ]
  },
  "keywords": [
    "dependencies",
    "detect",
    "blackduck",
    "black duck",
    "synopsys",
    "extract",
    "retrieve"
  ]
}